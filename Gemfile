gem "jekyll-remote-theme"
gem "minimal-mistakes-jekyll"
gem "webrick"
gem 'wdm', '~> 0.1.1', :install_if => Gem.win_platform?

platforms :mingw, :x64_mingw, :mswin, :jruby do
    gem "tzinfo", ">= 1", "< 3"
    gem "tzinfo-data"
end
